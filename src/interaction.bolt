function ~/headphones_give:
    give @s minecraft:carved_pumpkin[minecraft:custom_model_data=48185,minecraft:item_name=Headphones,minecraft:rarity=rare,minecraft:max_stack_size=1,minecraft:lore=['{text:"Equip in your head slot to",italic:false,color:gray}','{text:"hear music everywhere!",italic:false,color:gray}']] 1
    execute unless entity @s[tag=nbs_headphones_given] run scoreboard players add nbs_stats headphones_players 1
    scoreboard players add nbs_stats headphones_given 1
    tag @s add nbs_headphones_given
    advancement revoke @s only nbs:headphones_left
    advancement revoke @s only nbs:headphones_right
